{
    "name": "spinen/laravel-version",
    "description": "Helpers to keep track of the version of your Laravel application.",
    "keywords": [
        "laravel",
        "library",
        "spinen",
        "version"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Jimmy Puckett",
            "email": "jimmy.puckett@spinen.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "illuminate/console": "5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/container": "5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/routing": "5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/support": "5.5.*|5.6.*|5.7.*|5.8.*",
        "illuminate/view": "5.5.*|5.6.*|5.7.*|5.8.*"
    },
    "require-dev": {
        "mikey179/vfsstream": "^1.6",
        "mockery/mockery": "^1",
        "phpunit/phpunit": "~7.0.1|~8.0",
        "psy/psysh": "^0.5.1",
        "symfony/thanks": "^1.0",
        "symfony/var-dumper": "~3.0"
    },
    "autoload": {
        "psr-4": {
            "Spinen\\Version\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spinen\\Version\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spinen\\Version\\VersionServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
